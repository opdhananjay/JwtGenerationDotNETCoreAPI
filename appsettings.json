{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "very_strong_secret_key_strong_strong", // use strong unique key
    "Issuer": "https://myapp.com",          // your application url or name
    "Audience": "myapip",                   // the audience your token intended for
    "ExpiryMinutes": 60                     // Expiration time in minutes
  }
}
